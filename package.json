  "scripts": {
    "build:baseball:preview-local": "NODE_ENV=development webpack --config ./baseballs/preview/preview-local.config.js",
    "build:baseball:preview-remote": "NODE_ENV=production webpack --config ./baseballs/preview/preview-remote.config.js",
    "build:baseball:presentation": "NODE_ENV=production webpack --config ./baseballs/presentation/presentation-remote.config.js",
    "build:product:development": "NODE_ENV=development webpack --config ./src/product/webpack/development.config.js",
    "build:product:production": "NODE_ENV=production webpack --config ./src/product/webpack/production.config.js",
    "watch:baseball:preview-local": "NODE_ENV=development webpack --config ./baseballs/preview/preview-local.config.js --watch",
    "watch:product:development": "NODE_ENV=development webpack --config ./src/product/webpack/development.config.js --watch",
    "start": "npm run watch:development",
    "stats:product": "NODE_ENV=production webpack --config ./src/product/webpack/production.config.js --profile --json=www/product/stats.json",
    "build:development": "npm-run-all build:baseball:preview-local build:product:development",
    "build:production": "npm-run-all build:baseball:preview-remote build:product:production",
    "watch:development": "npm-run-all --parallel watch:baseball:preview-local watch:product:development"
  },
