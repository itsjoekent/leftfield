{
  "name": "app.renderer",
  "scripts": {
    "build-all-frontends": "npm-run-all build:frontend:*",
    "build:frontend:preview-local": "NODE_ENV=development webpack --config ./frontends/preview/preview-local.config.js",
    "build:frontend:preview-remote": "NODE_ENV=production webpack --config ./frontends/preview/preview-remote.config.js",
    "build:frontend:presentation": "NODE_ENV=production webpack --config ./frontends/presentation/presentation-remote.config.js",
    "watch:frontend:preview-local": "NODE_ENV=development webpack --config ./frontends/preview/preview-local.config.js --watch",
    "start": "npm-run-all --parallel watch:frontend:preview-local start:local",
    "start:local": "NODE_ENV=development node src/index",
    "start:remote": "NODE_ENV=production node src/index"
  },
  "dependencies": {
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "core-js": "^3.15.2",
    "css-loader": "^6.2.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.3.2",
    "lodash.get": "^4.4.2",
    "md5": "^2.3.0",
    "pino": "^6.11.3",
    "pino-http": "^5.5.0",
    "pkg.builder": "file:../../packages/builder",
    "pkg.campaign-components": "file:../../packages/campaign-components",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.7",
    "style-loader": "^3.2.1",
    "webpack": "^5.41.1",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "serve": "^12.0.0",
    "webpack-cli": "^4.7.2"
  }
}
